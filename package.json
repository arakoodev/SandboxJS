{
  "name": "@nyariv/sandboxjs",
  "version": "0.5.3",
  "description": "Javascript sandboxing library.",
  "main": "dist/Sandbox.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
